{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [ { "Name": "Console" } ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Data Source=database/app.db"
  },
  "Spotify": {
    "AccountsApiBaseUrl": "https://accounts.spotify.com/api",
    "WebApiBaseUrl": "https://api.spotify.com/v1"
  }
}